{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts | pino-pretty",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "gen:swagger": "tsp --config ./swagger/tspconfig.yaml compile ./swagger",
    "gen:schemas": "rimraf src/schemas && kubb --config kubb.config.ts && rimraf src/schemas/schemas",
    "deps:check": "npx npm-check-updates",
    "deps:upd": "npx npm-check-updates -u && pnpm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@kubb/cli": "^2.25.1",
    "@kubb/core": "^2.25.1",
    "@kubb/plugin-oas": "^2.25.1",
    "@kubb/swagger": "^2.25.1",
    "@kubb/swagger-ts": "^2.25.1",
    "@kubb/swagger-zod": "^2.25.1",
    "@typespec/compiler": "^0.58.1",
    "@typespec/http": "^0.58.0",
    "@typespec/openapi": "^0.58.0",
    "@typespec/openapi3": "^0.58.0",
    "@typespec/rest": "^0.58.0",
    "@typespec/versioning": "^0.58.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "mongoose": "^8.5.2",
    "pino": "^9.3.2",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@vitest/ui": "^2.0.5",
    "eslint": "^9.8.0",
    "pino-pretty": "^11.2.2",
    "supertest": "^7.0.0",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
